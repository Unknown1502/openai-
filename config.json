{
  "api_key": null,
  "api_base": null,
  "model": "openai/gpt-oss-20b",
  "backend": "hf_local",
  "max_concurrent": 5,
  "rate_limit_per_min": 100,
  "cache_enabled": false,
  "cache_dir": "outputs/cache",
  "data_dir": "data",
  "outputs_dir": "outputs",
  "templates_dir": "src/templates",
  "request_timeout_seconds": 60,
  "retry_attempts": 3,
  "retry_backoff_seconds": 1.0,
  "hf_max_new_tokens": 512,
  "hf_do_sample": true,
  "hf_top_p": 0.95,
  "hf_temperature": 0.7,
  "hf_torch_dtype": "float16",
  "hf_device_map": "auto",
  "hf_reasoning_effort": "medium",
  "hf_load_in_8bit": true,
  "hf_load_in_4bit": false,
  "hf_bnb_4bit_compute_dtype": "float16",
  "hf_bnb_4bit_quant_type": "nf4",
  "hf_bnb_4bit_use_double_quant": true,
  "hf_max_memory": {"0": "20GB", "cpu": "40GB"}
}
